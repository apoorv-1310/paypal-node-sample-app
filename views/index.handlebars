<h4>Plans</h4>
<div class="row">
  {{billingPlan.length}}
  {{#if plan_exists }}
    {{#each billingPlans}}
      <div class="col-md-4 col-md-offset-2 alert-success" style="padding: 20px;">
        {{@index}}
        <p>PlanId:{{{this.id}}}</p>
        <p>Plan status:{{{this.state}}}</p>
        <p>Plan Name:{{{this.name}}}</p>
        <p><a href="/plan_details?plan_id={{{this.id}}}">Get Details</a></p>
        <p><a href="/cancel_plan?plan_id={{{this.id}}}">Cancel</a></p>
      </div>
     {{/each}}
  {{else}}
    <p class="alert alert-danger">No Plan Exists</p>
  {{/if}}
</div>
